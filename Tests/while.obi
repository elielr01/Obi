play(){

    Int i = 1;
    while(i <= 5){
        print("HELLO WORLD!");
        i = i + 1;
    }

    print("");

    i = 1;
    Int j;
    String s;
    while ( i <= 5 ) {
        j = 1;
        s = "";
        while ( j <= 5 ) {
            s = s + "HELLO WORLD!!    ";
            j = j + 1;
        }
        print(s);
        i = i + 1;
    }
}