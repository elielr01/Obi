func printHello(Int n){

    if( n > 0) {
        print("hello world");
        printHello(n - 1);
    }
}

func fibonacci(Int n){

    Int iter = n;
    Int acum = 1;

    print(acum);

    while(iter >= 1){
        acum = acum * iter;
        print(acum);
        iter = iter - 1;
    }

    print(acum);

}


