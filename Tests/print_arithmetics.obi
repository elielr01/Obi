play(){
    #-
    # Printing a resultant int expression
    print(10 + 20); # 30
    print(200 - 150); #50
    print(10 * 2); # 20
    print(9 / 2); # 4
    print(9 % 2); # 1

    print(10 * 2 - 9 / 2); # 16
    print(10 * 4 / 2); # 20

    # Printing a resultant float expression
    print(5.57 + 1.03); # 6.6
    print(0.3 - 0.15); # 0.15
    print(0.5 * 2); # 1.0
    print(1.5 / 2); #0.75
    print(4.5 % 2.5); #2.0

    print(2.5 * 4 + 2 / 1.5); #11.3333333334
    print(3.4 * 1.2 / 2.1); # 1.9428571428571428
    -#

    #-
    # Printing negatives
    print(-5); # -5
    print(-10 - 5); # -15
    print(10 - -5); # 15
    print(10 - 5); # 5
    -#

    # Printing math with negative numbers
    print(10 * -5); # -50
    print(10 / -2 * -2.5); #12.5
    print(-10);
}